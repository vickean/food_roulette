<div class="col-lg-4 col-lg-offset-4 text-center">
 <h2>SIGN UP </h2><hr class="colorgraph">
 <div class="form-group">
 <%= form.label :first_name, "First_name" %>
 <%= form.text_field :first_name, class: "form-control input-lg"%>
</div><div class="form-group">
 <%= form.label :last_name, "Last_name" %>
 <%= form.text_field :last_name, class: "form-control input-lg"%>
</div><div class="form-group">
 <%= form.label :gender, "Gender" %><br>
 <%= form.select :gender, [['Male', 'M'], ['Female', 'F']], class: "form-control input-lg"%>
</div><div class="form-group">
 <%= form.label :email %>
 <%= form.text_field :email, type: 'email', class: "form-control input-lg" %>
</div><div class="form-group">
 <%= form.label :password %>
 <%= form.password_field :password, class: "form-control input-lg"%>
</div><div class="form-group">
<%= form.label :picture %>
<%= form.file_field :avatar, class: "form-control input-lg"%>
</div>
